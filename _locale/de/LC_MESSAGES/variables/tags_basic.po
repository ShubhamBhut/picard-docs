msgstr ""

#: ../../variables/tags_basic.rst:252
msgid "Total number of discs in this release"
msgstr ""

#: ../../variables/tags_basic.rst:254
msgid "**totaltracks**"
msgstr ""

#: ../../variables/tags_basic.rst:256
msgid "Total tracks on this disc."
msgstr ""

#: ../../variables/tags_basic.rst:258
msgid "**tracknumber**"
msgstr ""

#: ../../variables/tags_basic.rst:260
msgid "Track number on the disc."
msgstr ""

