#: ../../source/technical/command_line_pdf.rst:7
msgid "Appendix 3: :index:`Command Line Options <pair: command line; options>`"
msgstr ""

#: ../../source/technical/command_line_common.txt:9
msgid "Picard can be started from the command line with the following arguments:"
msgstr ""

#: ../../source/technical/command_line_common.txt:15
msgid "where the options are:"
msgstr ""

#: ../../source/technical/command_line_common.txt:17
msgid "``-h``, ``--help`` show a help message and exit"
msgstr ""

#: ../../source/technical/command_line_common.txt:19
msgid "``-c CONFIG_FILE``, ``--config-file CONFIG_FILE`` location of the configuration file to use"
msgstr ""

#: ../../source/technical/command_line_common.txt:21
msgid "``-d``, ``--debug`` enable debug-level logging"
msgstr ""

#: ../../source/technical/command_line_common.txt:23
msgid "``-M``, ``--no-player`` disable built-in media player"
msgstr ""

#: ../../source/technical/command_line_common.txt:25
msgid "``-N``, ``--no-restore`` do not restore window positions or sizes"
msgstr ""

#: ../../source/technical/command_line_common.txt:27
msgid "``-P``, ``--no-plugins`` do not load any plugins"
msgstr ""

#: ../../source/technical/command_line_common.txt:29
msgid "``-v``, ``--version`` display the version information and exit"
msgstr ""

#: ../../source/technical/command_line_common.txt:31
msgid "``-V``, ``--long-version`` display the long version information and exit"
msgstr ""

#: ../../source/technical/command_line_common.txt:33
msgid "``FILE`` is the file or files to load.  If one of the filenames begins with a hyphen, use ``--`` to separate the options from the filenames."
msgstr ""
