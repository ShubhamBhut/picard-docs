#: ../../source/variables/variables_basic.rst:11
msgid ":index:`Basic Variables <variables; basic>`"
msgstr ""

#: ../../source/variables/variables_basic.rst:13
msgid "These variables are populated from MusicBrainz data for most releases, without any special Picard settings."
msgstr ""

#: ../../source/variables/variables_basic.rst:17
msgid "Variables will not be created if there was no value retrieved for the variable from the MusicBrainz database."
msgstr ""

#: ../../source/variables/variables_basic.rst:19
msgid "**_absolutetracknumber**"
msgstr ""

#: ../../source/variables/variables_basic.rst:21
msgid "The absolute number of this track disregarding the disc number (i.e.: ``%_absolutetracknumber%`` of ``%_totalalbumtracks%``). For example, this value would be 11 for the second track on disc 2 where disc 1 has 9 tracks (*since Picard 1.3*)."
msgstr ""

#: ../../source/variables/variables_basic.rst:24
msgid "**_albumartists**"
msgstr ""

#: ../../source/variables/variables_basic.rst:26
msgid "The Album's Artists' Name(s) (multi-value) (*since Picard 1.3*)."
msgstr ""

#: ../../source/variables/variables_basic.rst:28
msgid "**_albumartists_sort**"
msgstr ""

#: ../../source/variables/variables_basic.rst:30
msgid "The Album Artist's Sort Name(s) (multi-value) (*since Picard 1.3*)."
msgstr ""

#: ../../source/variables/variables_basic.rst:32
msgid "**_artists_sort**"
msgstr ""

#: ../../source/variables/variables_basic.rst:34
msgid "The Artist's Sort Name(s) (multi-value) (*since Picard 1.3*)."
msgstr ""

#: ../../source/variables/variables_basic.rst:36
msgid "**_datatrack**"
msgstr ""

#: ../../source/variables/variables_basic.rst:38
msgid "Set to 1 if the track is a \"`data track <https://wiki.musicbrainz.org/Style/Unknown_and_untitled/Special_purpose_track_title#Data_tracks>`_\" (*since Picard 1.3.1*)."
msgstr ""

#: ../../source/variables/variables_basic.rst:40
msgid "**_discpregap**"
msgstr ""

#: ../../source/variables/variables_basic.rst:42
msgid "Set to 1 if the disc the track is on has a \"`pregap track <https://musicbrainz.org/doc/Terminology#hidden_track>`_\" (*since Picard 1.4*)."
msgstr ""

#: ../../source/variables/variables_basic.rst:44
msgid "**_multiartist**"
msgstr ""

#: ../../source/variables/variables_basic.rst:46
msgid "0 if tracks on the album all have the same primary artist, 1 otherwise (*since Picard 1.3*)."
msgstr ""

#: ../../source/variables/variables_basic.rst:48
msgid "**_pregap**"
msgstr ""

#: ../../source/variables/variables_basic.rst:50
msgid "Set to 1 if the track is a \"`pregap track <https://musicbrainz.org/doc/Terminology#hidden_track>`_\" (*since Picard 1.3.1*)."
msgstr ""

#: ../../source/variables/variables_basic.rst:54
msgid "**_primaryreleasetype**"
msgstr ""

#: ../../source/variables/variables_basic.rst:56
msgid "Release Group Primary type (i.e.: album, single, ep, broadcast, or other)."
msgstr ""

#: ../../source/variables/variables_basic.rst:58
msgid "**_rating**"
msgstr ""

#: ../../source/variables/variables_basic.rst:60
msgid "Rating 0-5 by MusicBrainz users."
msgstr ""

#: ../../source/variables/variables_basic.rst:62
msgid "**_releasecomment**"
msgstr ""

#: ../../source/variables/variables_basic.rst:64
msgid "Release disambiguation comment (*since Picard 0.15*)."
msgstr ""

#: ../../source/variables/variables_basic.rst:66
msgid "**_releasegroup**"
msgstr ""

#: ../../source/variables/variables_basic.rst:68
msgid "Release Group Title which is typically the same as the Album Title, but can be different."
msgstr ""

#: ../../source/variables/variables_basic.rst:70
msgid "**_releasegroupcomment**"
msgstr ""

#: ../../source/variables/variables_basic.rst:72
msgid "Release Group disambiguation comment."
msgstr ""

#: ../../source/variables/variables_basic.rst:74
msgid "**_releaselanguage**"
msgstr ""

#: ../../source/variables/variables_basic.rst:76
msgid "Release Language as per `ISO 639-3 <https://en.wikipedia.org/wiki/ISO_639-3>`_ (*since Picard 0.10*)."
msgstr ""

#: ../../source/variables/variables_basic.rst:80
msgid "**_secondaryreleasetype**"
msgstr ""

#: ../../source/variables/variables_basic.rst:82
msgid "Zero or more Release Group Secondary types (i.e.: audiobook, compilation, dj-mix, interview, live, mixtape/street, remix, soundtrack, or spokenword)."
msgstr ""

#: ../../source/variables/variables_basic.rst:84
msgid "**_totalalbumtracks**"
msgstr ""

#: ../../source/variables/variables_basic.rst:86
msgid "The total number of tracks across all discs of this release."
msgstr ""
